{
  "name": "media-manager",
  "version": "1.0.0",
  "description": "A manager for all the media and entertaining stuff in your home as movies, series, music, video games and so on.",
  "main": "app.js",
  "scripts": {
    "start": "node src/server/server.js",
    "dev": "nodemon src/server/server.js --ext js,graphql --exec babel-node",
    "test": "mocha **/*.test.js",
    "test-watch": "nodemon --exec \"npm test\"",
    "test-jest": "jest --verbose --coverage --watchAll"
  },
  "engines": {
    "node": "9.5.0"
  },
  "author": "Marco Braun",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "graphql-yoga": "^1.17.4",
    "hbs": "^4.0.1",
    "moment": "^2.22.2",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "supertest": "^3.3.0"
  }
